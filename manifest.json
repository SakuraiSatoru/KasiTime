{
	"manifest_version": 2,
	"name": "Kasi Time Copyable",
	"version": "0.0.1",
	"background": { "scripts": ["background.js"] },
	"page_action": {
		"default_icon": {
			"38": "38.png",
			"19": "19.png"
		},
		"default_title": "Kasi Time Prompt", // shown in tooltip
		"default_popup": "popup.html"
	},
	"permissions" : ["tabs"],
	"content_scripts":[{
		"matches":["http://www.kasi-time.com/*"],
		"js":["jquery-2.0.0.min.js", "content_script.js"]
	}]
}
